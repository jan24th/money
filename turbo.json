{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "dependsOn": [
        "^dev"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ]
    },
    "deploy": {
      "dependsOn": [
        "^deploy"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    }
  }
}
